@orders.map do |order|
  {
    id: order.id,
    user_id: order.user_id,
    product_id: order.product_id,
    quantity: order.quantity,
    subtotal: order.subtotal,
    tax: order.tax,
    total: order.total,
    product: order.product,
    created_at: order.created_at,
    updated_at: order.updated_at
  }
end
